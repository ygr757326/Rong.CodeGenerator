import { ${titleCase(folderName)}GetListItem, ${titleCase(
    folderName
  )}GetListParams, ${titleCase(
    folderName
  )}GetListResult } from './model/${titleCase(folderName)}Model';
import { commonParams } from './model/identityModel';
import { defHttp } from '@/utils/http/axios';
enum Api {
  //获取驾驶员信息列表
  ${titleCase(folderName)}GetList = '/api/SafetySupervise/${firstLowCase(
    titleCase(folderName)
  )}/getList',
  // 获取驾驶员信息详情
  ${titleCase(folderName)}Get = '/api/SafetySupervise/${firstLowCase(
    titleCase(folderName)
  )}/get',
  // 修改驾驶员信息
  ${titleCase(folderName)}Update = '/api/SafetySupervise/${firstLowCase(
    titleCase(folderName)
  )}/update?id=',
  // 删除驾驶员信息
  ${titleCase(folderName)}Delete = '/api/SafetySupervise/${firstLowCase(
    titleCase(folderName)
  )}/delete?id=',
  //创建驾驶员信息
  ${titleCase(folderName)}Create = '/api/SafetySupervise/${firstLowCase(
    titleCase(folderName)
  )}/create',
}

// 获取驾驶员信息列表
export const ${titleCase(folderName)}GetList = (params?: ${titleCase(
    folderName
  )}GetListParams) =>
  defHttp.get<${titleCase(folderName)}GetListResult>({
    url: Api.${titleCase(folderName)}GetList,
    params,
  });
// 获取驾驶员信息详情
export const ${titleCase(folderName)}Get = (params: commonParams) =>
  defHttp.get<${titleCase(folderName)}GetListItem>({
    url: Api.${titleCase(folderName)}Get,
    params,
  });
// 修改驾驶员信息
export const ${titleCase(folderName)}Update = (id, data: ${titleCase(
    folderName
  )}GetListItem) =>
  defHttp.put<${titleCase(folderName)}GetListItem>({
    url: Api.${titleCase(folderName)}Update + id,
    data,
  });
// 删除驾驶员信息
export const ${titleCase(folderName)}Delete = (id: string) =>
  defHttp.delete({
    url: Api.${titleCase(folderName)}Delete + id,
  });
// 创建驾驶员信息
export const ${titleCase(folderName)}Create = (params: ${titleCase(
    folderName
  )}GetListItem) =>
  defHttp.post({
    url: Api.${titleCase(folderName)}Create,
    params,
  });
