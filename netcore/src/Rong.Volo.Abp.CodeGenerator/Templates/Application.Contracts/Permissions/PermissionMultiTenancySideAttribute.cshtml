@using Rong.Volo.Abp.CodeGenerator.Models
@inherits Volo.Abp.TextTemplating.Razor.RazorTemplatePageBase<Models.TemplateModel>

using System;
using Volo.Abp.MultiTenancy;

namespace @{@Model.NameSpace}.Permissions
{
    /// <summary>
    /// 权限多租户
    /// </summary>
    [AttributeUsage(AttributeTargets.Class | AttributeTargets.Field)]
    public class PermissionMultiTenancySideAttribute : Attribute
    {
        public MultiTenancySides Side { get; }

        public PermissionMultiTenancySideAttribute()
        {
            Side = MultiTenancySides.Both;
        }

        public PermissionMultiTenancySideAttribute(MultiTenancySides side)
        {
            Side = side;
        }
    }
}

